{
  "name": "moda-sdk",
  "keywords": [
    "MODA",
    "Music3",
    "Music",
    "Distribution",
    "Royalties",
    "NFTs",
    "Polygon",
    "Ethereum",
    "Web3"
  ],
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "description": "A JavaScript library for MODA protocol",
  "license": "Apache-2.0",
  "repository": "github:modadao/moda-protocol",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "tsc",
    "release": "pnpm run build && changeset publish",
    "test": "jest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.4",
    "@wagmi/cli": "^2.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./metadata": {
      "import": {
        "types": "./dist/metadata.d.ts",
        "default": "./dist/metadata.mjs"
      },
      "require": {
        "types": "./dist/metadata.d.ts",
        "default": "./dist/metadata.js"
      }
    },
    "./profile": {
      "import": {
        "types": "./dist/profile.d.ts",
        "default": "./dist/profile.mjs"
      },
      "require": {
        "types": "./dist/profile.d.ts",
        "default": "./dist/profile.js"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.mjs"
      },
      "require": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.mjs"
      },
      "require": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      }
    }
  },
  "dependencies": {
    "@wagmi/core": "^2.2.1",
    "codegen": "link:@wagmi/core/codegen",
    "ts-node": "^10.9.2",
    "viem": "^1.19.11"
  }
}
